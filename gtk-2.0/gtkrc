		#} 
	}
}

style "styleNotebook" {
	#thickness leads to the selected notebook getting drawn larger.. (aha?)
	xthickness = 2
	ythickness = 2
  	GtkNotebook::tab-overlap = 4
	#some pixel trouble in this department but how to solve...?
	engine "pixmap" 
	{
		#this seems to be unnecessary (or defined also elswhere)
		image {
			function = BOX
			file = "resources/notebook.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
		      }
		image {
			function = EXTENSION
			state = ACTIVE
			file = "resources/notebookTabTop.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_side = BOTTOM
		}
		image {
			function = EXTENSION
			file = "resources/notebookTabTopActive.png"
			border = {1, 1, 1, 1}
			stretch = TRUE
			gap_side = BOTTOM
		}
		image {
			function = EXTENSION
			state = ACTIVE
			file = "resources/notebookTabBottom.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_side = TOP
		}
		image {
			function = EXTENSION
			file = "resources/notebookTabBottomActive.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_side = TOP
		}
		image {
			function = EXTENSION
			state = ACTIVE
			file = "resources/notebookTabLeft.png"
			border = {1,1,1,1}
			stretch	= TRUE
			gap_side = RIGHT
		}
		image {
			function = EXTENSION
			file = "resources/notebookTabLeftActive.png"
			border = {1, 1, 1, 1}
			stretch = TRUE
			gap_side = RIGHT
		}
    		image {
			function = EXTENSION
			state = ACTIVE
			file = "resources/notebookTabRight.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_side = LEFT
		}
		image {
			function = EXTENSION
			file = "resources/notebookTabRightActive.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_side = LEFT
		}
		#note this represents open area in the box not in the tab
		#if this is not present, the entire box is drawn by default
		#drawer not by pixmap. the pixmaps in the box then have no 
		#effect
		image {
			function = BOX_GAP
			file = "resources/notebook.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_file = "resources/notebookTopGap.png"
			gap_border = {1, 1, 0, 0}
			gap_start_file = "null.png"
			gap_start_border = {0, 0, 0, 0}
			gap_end_file = "null.png"
			gap_end_border = {0, 0, 0, 0}
			gap_side = TOP
		}
		image {
			function = BOX_GAP
			file = "resources/notebook.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_file = "resources/notebookBottomGap.png"
			gap_border = {1, 1, 0, 0}
			gap_start_file = "null.png"
			gap_start_border = {0, 0, 0, 0}
			gap_end_file = "null.png"
			gap_end_border = {0, 0, 0, 0}
			gap_side = BOTTOM
		      }
		image {
			function = BOX_GAP
			file = "resources/notebook.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_file = "resources/notebookLeftGap.png"
			gap_border = {0, 0, 1, 1}
			gap_start_file = "null.png"
			gap_start_border = {0, 0, 0, 0}
			gap_end_file = "null.png"
			gap_end_border = {0, 0, 0, 0}
			gap_side = LEFT
      		}
		image {
			function = BOX_GAP
			file = "resources/notebook.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_file = "resources/notebookRightGap.png"
			gap_border = {0, 0, 1, 1}
			gap_start_file = "null.png"
			gap_start_border = {0, 0, 0, 0}
			gap_end_file = "null.png"
			gap_end_border = {0, 0, 0, 0}
			gap_side = RIGHT
		}
		#image {
			#function = FOCUS
			#border = {1, 1, 1, 1}
			#stretch = TRUE
		#}
	}
}



style "styleNotebook6" {
	#thickness leads to the selected notebook getting drawn larger.. (aha?)
	xthickness = 2
	ythickness = 2
  	GtkNotebook::tab-overlap = 4

	#some pixel trouble in this department but how to solve...?
	engine "pixmap" 
	{
		#this seems to be unnecessary (or defined also elswhere)
		image {
			function = BOX
			file = "resources/dark/notebook.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
		      }
		image {
			function = EXTENSION
			state = ACTIVE
			file = "resources/dark/notebookTabTop.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_side = BOTTOM
		}
		image {
			function = EXTENSION
			file = "resources/dark/notebookTabTopActive.png"
			border = {1, 1, 1, 1}
			stretch = TRUE
			gap_side = BOTTOM
		}
		image {
			function = EXTENSION
			state = ACTIVE
			file = "resources/dark/notebookTabBottom.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_side = TOP
		}
		image {
			function = EXTENSION
			file = "resources/dark/notebookTabBottomActive.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_side = TOP
		}
		image {
			function = EXTENSION
			state = ACTIVE
			file = "resources/dark/notebookTabLeft.png"
			border = {1,1,1,1}
			stretch	= TRUE
			gap_side = RIGHT
		}
		image {
			function = EXTENSION
			file = "resources/dark/notebookTabLeftActive.png"
			border = {1, 1, 1, 1}
			stretch = TRUE
			gap_side = RIGHT
		}
    		image {
			function = EXTENSION
			state = ACTIVE
			file = "resources/dark/notebookTabRight.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_side = LEFT
		}
		image {
			function = EXTENSION
			file = "resources/dark/notebookTabRightActive.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_side = LEFT
		}
		#note this represents open area in the box not in the tab
		#if this is not present, the entire box is drawn by default
		#drawer not by pixmap. the pixmaps in the box then have no 
		#effect
		image {
			function = BOX_GAP
			file = "resources/dark/notebook.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_file = "resources/dark/notebookTopGap.png"
			gap_border = {1, 1, 0, 0}
			gap_start_file = "null.png"
			gap_start_border = {0, 0, 0, 0}
			gap_end_file = "null.png"
			gap_end_border = {0, 0, 0, 0}
			gap_side = TOP
		}
		image {
			function = BOX_GAP
			file = "resources/dark/notebook.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_file = "resources/dark/notebookBottomGap.png"
			gap_border = {1, 1, 0, 0}
			gap_start_file = "null.png"
			gap_start_border = {0, 0, 0, 0}
			gap_end_file = "null.png"
			gap_end_border = {0, 0, 0, 0}
			gap_side = BOTTOM
		      }
		image {
			function = BOX_GAP
			file = "resources/dark/notebook.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_file = "resources/dark/notebookLeftGap.png"
			gap_border = {0, 0, 1, 1}
			gap_start_file = "null.png"
			gap_start_border = {0, 0, 0, 0}
			gap_end_file = "null.png"
			gap_end_border = {0, 0, 0, 0}
			gap_side = LEFT
      		}
		image {
			function = BOX_GAP
			file = "resources/dark/notebook.png"
			border = {1, 1, 1, 1}
			stretch	= TRUE
			gap_file = "resources/dark/notebookRightGap.png"
			gap_border = {0, 0, 1, 1}
			gap_start_file = "null.png"
			gap_start_border = {0, 0, 0, 0}
			gap_end_file = "null.png"
			gap_end_border = {0, 0, 0, 0}
			gap_side = RIGHT
		}
		#image {
			#function = FOCUS
			#border = {1, 1, 1, 1}
			#stretch = TRUE
		#}
	}
}


style "styleButton" {
	engine "pixmap" {
		image {
			function = BOX
			state = NORMAL
			file = "resources/button.png"
			border = {3, 3, 3, 3}
			stretch = TRUE
		}
		image {
			function = BOX
			state = PRELIGHT
			file = "resources/button.png"
			border = {3, 3, 3, 3}
			stretch = TRUE
		}
		image {
			function = BOX
			state = INSENSITIVE
			file = "resources/button.png"
			border = {3, 3, 3, 3}
			stretch = TRUE
		}
		image {
			function = BOX
			state = ACTIVE
			file = "resources/buttonPressed.png"
			border = {3, 3, 3, 3}
			stretch = TRUE
		}
		#image {
			#function = FOCUS
		#}
	}
}

style "styleButton6" {
