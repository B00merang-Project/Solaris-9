style "styleFrame" {
	engine "pixmap" {
		image {
			function = SHADOW
			shadow   = NONE
		}
		#these two affect the statusbar but oddly enough not the
		#'frame in and out' in awf-gtk2
		image {
			function	= SHADOW
			recolorable	= FALSE
			shadow		= IN
			file     = "resources/frameSHADOWIn.png"
			border		= { 2, 2, 2, 2}
			stretch		= TRUE
		}
		image {
			function	= SHADOW
			recolorable	= FALSE
			shadow		= OUT
			file     = "resources/frameSHADOWOut.png"
			border		= { 2, 2, 2, 2 }
			stretch		= TRUE
		}   
		image {
			function	= SHADOW
			recolorable	= FALSE
			shadow		= ETCHED_IN
			file     = "resources/frameStandAloneOut.png"
			border		= { 2, 2, 2, 2}
			stretch		= TRUE
		}
		image {
			function	= SHADOW
			recolorable	= FALSE
			shadow		= ETCHED_OUT
			file     = "resources/frameStandAlone.png"
			border		= { 2, 2, 2, 2 }
			stretch		= TRUE
		}
		#WITHOUT THIS THE FRAMES ARE DRAWN BY DEFAULT ENGINE, ALL IMAGES ARE SKIPPED!
		#or is this the actual frame?
		#then what do the other 'etched out' etc do?
		image {
			function	= SHADOW_GAP
			recolorable	= TRUE
			file		= "resources/frameBoxInset.png"
			border		= { 2, 2, 2, 2 }
			stretch		= TRUE
			gap_start_file  = "resources/frameGapTopStart.png"
			gap_start_border= { 1, 0, 0, 0 }
			gap_end_file = "resources/frameGapTopEnd.png"
			gap_end_border  = { 0, 2, 0, 0 }
			gap_side	= TOP
		}
	}
}

