		    arrow_direction = LEFT
		}
		image {
		    function = ARROW
		    state = ACTIVE
		    overlay_file = "resources/dark/scrollbarArrowLeftPressed.png"
		    overlay_stretch = FALSE
		    arrow_direction = LEFT
		}
		image {
		    function = ARROW
		    state = NORMAL
		    overlay_file = "resources/dark/scrollbarArrowRight.png"
		    overlay_stretch = FALSE
		    arrow_direction = RIGHT
		}
		image {
		    function = ARROW
		    state = PRELIGHT
		    overlay_file = "resources/dark/scrollbarArrowRight.png"
		    overlay_stretch = FALSE
		    arrow_direction = RIGHT
		}
		image {
		    function = ARROW
		    state = INSENSITIVE
