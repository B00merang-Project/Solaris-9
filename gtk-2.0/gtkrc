			state		= INSENSITIVE
			recolorable	= TRUE
			overlay_file = "resources/dark/arrowDown.png"
			overlay_border	= { 0, 0, 0, 0 }
			overlay_stretch	= FALSE
			arrow_direction	= DOWN
		}
		image {
			function		= ARROW
			state		= NORMAL
			recolorable	= TRUE
			overlay_file = "resources/dark/arrowDown.png"
			overlay_border	= { 0, 0, 0, 0 }
			overlay_stretch	= FALSE
			arrow_direction	= LEFT
		}
		image {
			function		= ARROW
			state		= PRELIGHT
			recolorable	= TRUE
			overlay_file = "resources/dark/arrowDown.png"
			overlay_border	= { 0, 0, 0, 0 }
			overlay_stretch	= FALSE
			arrow_direction	= LEFT
		}
		image {
			function		= ARROW
			state		= ACTIVE
			recolorable	= TRUE
			overlay_file = "resources/dark/arrowDownPressed.png"
			overlay_border	= { 0, 0, 0, 0 }
			overlay_stretch	= FALSE
			arrow_direction	= LEFT
		}
		image {
			function		= ARROW
			state		= INSENSITIVE
			recolorable	= TRUE
			overlay_file = "resources/dark/arrowDown.png"
			overlay_border	= { 0, 0, 0, 0 }
			overlay_stretch	= FALSE
			arrow_direction	= LEFT
		}
		image {
			function		= ARROW
			state		= NORMAL
