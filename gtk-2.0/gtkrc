style "styleRadioButton" {
	engine "pixmap" {
		image {
			function = CHECK
			shadow = OUT
			overlay_file = "resources/radioButton.png"
			overlay_stretch = FALSE
		}
		image {
			function = CHECK
			shadow = IN
			overlay_file = "resources/radioButtonPressed.png"
			overlay_stretch = FALSE
		}
		image {
			function = OPTION
			shadow = OUT
