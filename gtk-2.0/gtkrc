		    function = ARROW
		    state = PRELIGHT
		    overlay_file = "resources/dark/scrollbarArrowDown.png"
		    overlay_stretch = FALSE
		    arrow_direction = DOWN
		}
		image {
		    function = ARROW
		    state = INSENSITIVE
		    overlay_file = "resources/dark/scrollbarArrowDown.png"
		    overlay_stretch = FALSE
		    arrow_direction = DOWN
		}
		image {
		    function = ARROW
		    state = ACTIVE
		    overlay_file = "resources/dark/scrollbarArrowDownPressed.png"
		    overlay_stretch = FALSE
		    arrow_direction = DOWN
		}
		image {
		    function = ARROW
		    state = NORMAL
		    overlay_file = "resources/dark/scrollbarArrowLeft.png"
		    overlay_stretch = FALSE
		    arrow_direction = LEFT
		}
		image {
		    function = ARROW
		    state = PRELIGHT
