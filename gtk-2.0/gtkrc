style "styleSpinButton"    {
	engine "pixmap" {
		image {
			function = BOX
			detail = "spinbutton"
			file = "resources/button.png"
			border = {4, 4, 4, 4}
			stretch	= TRUE
		}
		#???
		image {
			function = ARROW
		}
		image {
			function = BOX
			state = NORMAL
			detail = "spinbutton_up"
			overlay_file = "resources/spinButtonUp.png"
			#aha the image is 'overlayed' only transparent image with for black and white pixels
			#for transparent alpha=50overlay white corresponts to ts color black for bs color
			overlay_stretch	= FALSE
		}
		image {
			function = BOX
			state = PRELIGHT
      			detail = "spinbutton_up"
			overlay_file = "resources/spinButtonUp.png"
			overlay_stretch	= FALSE
		}
		image {
			function = BOX
			state = INSENSITIVE
			detail = "spinbutton_up"
			overlay_file = "resources/spinButtonUp.png"
			overlay_stretch	= FALSE
		}
		image {
			function = BOX
			state = ACTIVE
			detail = "spinbutton_up"
			overlay_file = "resources/spinButtonUpPressed.png"
			overlay_stretch	= FALSE
		}    
		image {
			function = BOX
			state = NORMAL
			detail = "spinbutton_down"
			overlay_file = "resources/spinButtonDown.png"
			overlay_stretch	= FALSE
		}
		image {
			function = BOX
			state = PRELIGHT
			detail = "spinbutton_down"
			overlay_file = "resources/spinButtonDown.png"
			overlay_stretch	= FALSE
		}
		image {
			function = BOX
			state = INSENSITIVE
			detail = "spinbutton_down"
			overlay_file = "resources/spinButtonDown.png"
			overlay_stretch	= FALSE
		}
		image {
			function = BOX
			state = ACTIVE
			detail = "spinbutton_down"
			overlay_file = "resources/spinButtonDownPressed.png"
			overlay_stretch	= FALSE
		}
  	}
}


